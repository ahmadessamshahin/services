{
  "name": "services",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@prettier/plugin-python": "0.0.0-development",
    "pinst": "^2.1.6",
    "pre-commit": "^1.2.2",
    "prettier": "2.3.0"
  },
  "dependencies": {
    "husky": "^6.0.0"
  },
  "private": false,
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
   "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
   "lint-staged": {
    "*": "prettier -- --write  '**/*.py'"
  }
}
